        <TableRow>

            <EditText
                android:id="@+id/etCodPaciente"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="@string/codPacIntegracao" />
        </TableRow>

        <TableRow
            android:id="@+id/TableRow03"
            android:layout_width="wrap_content"
