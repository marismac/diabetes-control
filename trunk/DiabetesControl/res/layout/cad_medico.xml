                android:width="100dp" />
        </TableRow>

        <TableRow>

            <EditText
                android:id="@+id/etEmail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="@string/emailmedico" />

        </TableRow>

        <TableRow
            android:id="@+id/TableRow02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView05"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </TableRow>

        <TableRow>

            <TextView
                android:id="@+id/TextView09"
                android:text="@string/registromedico"
                android:width="100dp" />
        </TableRow>

        <TableRow>

            <EditText
                android:id="@+id/etRegMedico"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="@string/registromedicohint" />
        </TableRow>

        <TableRow
            android:id="@+id/TableRow03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

